/* ---------- RENDER CARDS ---------- */
const cardHTML = r => `
<div class="card ${r.cat}" id="card${r.idx}">
  <span class="index">${r.idx}.</span>
  <a class="restLink" href="${r.url || '#'}" target="_blank">${r.name}</a>
  <span class="price">${r.price}</span><br>
  <span class="tags">${r.cuisine}</span><br>
  ${ r.blurb ? `<span class="blurb">${r.blurb}</span><br>` : "" }
  ${ r.dish  ? `<em>${r.dish}</em>` : "" }
  <!-- Google Rating will be injected here -->
</div>`;

const list = document.getElementById("cardsContainer");
// Added "bars" to the list of categories to display
["top","main","chinese","pizza","bagels","desserts","pastries","bars"].forEach(cat=>{
  list.insertAdjacentHTML("beforeend",`<h2>${{
    top:"💎 Top-Tier",main:"🥂 Mainstream",chinese:"🥟 Chinese / Dim Sum",
    pizza:"🍕 Pizza",bagels:"🥯 Bagels",desserts:"🍰 Desserts",
    pastries:"🥐 Bread & Pastries", bars:"🍸 Bars" // Added Bars Title
  }[cat]}</h2>`);
  list.insertAdjacentHTML("beforeend",data.filter(d=>d.cat===cat).map(cardHTML).join(""));
});


