window.highlight = i => { /* Interaction disabled for iframe map */ };
window.unhighlight = i => { /* Interaction disabled for iframe map */ };

document.getElementById("catFilter").addEventListener("change",()=>{
  const sel=document.getElementById("catFilter").value;
  data.forEach(d=>{
     const show = sel==="all" || d.cat===sel;
     const cardElement = document.getElementById("card"+d.idx);
     if (cardElement) {
         cardElement.style.display = show?"block":"none";
     }
     // Cannot filter the iframe map markers
  });
});

/********************************************************************
 *  Google Reviews  – look up each place, then inject stars & counts *
 ********************************************************************/
function renderStars(avg){           // returns ★★★★☆ span
  if (typeof avg !== 'number' || avg <= 0) return ''; // Don't render if no rating
  const full = Math.floor(avg);
  const half = avg - full >= 0.5; // Use 0.5 threshold for half star appearance
  let html = '<span style="color:#f4b400; font-size: 1.1em; line-height: 1;">'; // Style stars
  for(let i=0; i<5; i++){
    if(i < full) html+='★'; // full star ★
    else if(i === full && half) html+='★'; // Render half as full for simplicity or use a specific half-star character if desired e.g. '⯨' but may need font support
    else html+='☆'; // empty star ☆
  }
  html+='</span>';
  return html;
}

function injectRating(cardId, avg, total){
  const node = document.getElementById(cardId);
  if(!node || typeof avg !== 'number' || avg <= 0 || typeof total !== 'number' || total <= 0) return; // Check if node exists and ratings are valid numbers > 0

  // Format the rating number and review count
  const ratingText = avg.toFixed(1);
  const reviewCountText = total.toLocaleString(); // Add commas for thousands

  const html = `${renderStars(avg)} ${ratingText} (${reviewCountText} reviews)`;
  // Ensure the div holding the rating exists or create it
  let ratingDiv = node.querySelector('.google-rating');
  if (!ratingDiv) {
      ratingDiv = document.createElement('div');
      ratingDiv.className = 'google-rating';
      ratingDiv.style.marginTop = '4px';
      ratingDiv.style.fontSize = '0.85em'; // Smaller font for rating line
      node.appendChild(ratingDiv);
  }
  ratingDiv.innerHTML = html; // Update the content
}


// Ensure PlacesService is initialized only once
let gService;
try {
    gService = new google.maps.places.PlacesService(document.createElement('div'));
} catch (e) {
    console.error("Error initializing Google Places Service. Ensure the Google Maps API script loaded correctly.", e);
    return; // Stop execution if service fails
}

data.forEach(r=>{
  // Use LatLng for location bias if available, otherwise rely on query string
  const locationBias = (r.lat && r.lon) ? new google.maps.LatLng(r.lat, r.lon) : new google.maps.LatLng(HOME.lat, HOME.lon);
  const request = {
    query: r.name + ", New York, NY", // Make query more specific
    fields: ['name', 'rating', 'user_ratings_total', 'place_id'], // Request specific fields
    locationBias: locationBias, // Bias search towards the item's location
    // radius: 500 // Radius is less effective with locationBias in textSearch, but can be added
  };

  if (gService && typeof gService.textSearch === 'function') {
      gService.textSearch(request,(results, status)=>{
          if (status === google.maps.places.PlacesServiceStatus.OK && results && results[0]) {
              const place = results[0];
              r.placeId = place.place_id;
              r.rating  = place.rating;
              r.reviews = place.user_ratings_total;
              // Check if rating and reviews are valid numbers before injecting
              if (typeof r.rating === 'number' && typeof r.reviews === 'number') {
                  injectRating("card"+r.idx, r.rating, r.reviews);
              } else {
                   console.log(`No valid rating/reviews found for ${r.name} (ID: ${r.idx})`);
              }
          } else if (status === google.maps.places.PlacesServiceStatus.ZERO_RESULTS) {
              console.log(`Zero results found for ${r.name} (ID: ${r.idx})`);
          } else {
              console.error(`PlacesService textSearch failed for ${r.name} (ID: ${r.idx}) with status: ${status}`);
          }
      });
  } else {
       console.error("Google Places Service or textSearch method not available.");
  }
});

// Initial filter application for the list AFTER data setup
document.getElementById("catFilter").dispatchEvent(new Event('change'));
} // End of initPage function

